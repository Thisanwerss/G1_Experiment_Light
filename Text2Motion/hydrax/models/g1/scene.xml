      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
  </asset>

  <worldbody>
    <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>
    <body name="vicon_frame" pos="0 0 1.0">
        <geom name="vicon_x" type="capsule" fromto="0 0 0 0.2 0 0" size="0.015" rgba="1 0 0 1" contype="0" conaffinity="0"/>
        <geom name="vicon_y" type="capsule" fromto="0 0 0 0 0.2 0" size="0.015" rgba="0 1 0 1" contype="0" conaffinity="0"/>
        <geom name="vicon_z" type="capsule" fromto="0 0 0 0 0 0.2" size="0.015" rgba="0 0 1 1" contype="0" conaffinity="0"/>
    </body>
  
    <!-- <body name="box" pos="0.9 0.0 0.15">
